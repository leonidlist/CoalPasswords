<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:CoalPasswords">
    <LinearGradientBrush x:Key="PrimaryGradientBrush">
        <GradientStop Color="#667eea" Offset="0.2"></GradientStop>
        <GradientStop Color="#764ba2" Offset="0.8"></GradientStop>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="AccentGradientBrush">
        <GradientStop Color="#f093fb" Offset="0.2"></GradientStop>
        <GradientStop Color="#f5576c" Offset="0.8"></GradientStop>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="PrimaryBackgroundGradient">
        <GradientStop Color="#343746" Offset="0.1"></GradientStop>
        <GradientStop Color="#242430" Offset="0.6"></GradientStop>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="PrimaryBackgroundGradient2">
        <GradientStop Color="#485563" Offset="0.1"></GradientStop>
        <GradientStop Color="#29323c" Offset="0.58"></GradientStop>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="BlackTransparent" Color="Black" Opacity=".8"></SolidColorBrush>
    <SolidColorBrush x:Key="PrimaryForeground">White</SolidColorBrush>
</ResourceDictionary>