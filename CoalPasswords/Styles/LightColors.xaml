<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:CoalPasswords">
    <LinearGradientBrush StartPoint="0.5 0" EndPoint="0.5 1" x:Key="BackgroundGradient">
        <GradientStop Color="#f3e7e9" Offset="0.2"></GradientStop>
        <GradientStop Color="#e3eeff" Offset="0.7"></GradientStop>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="AccentColor">#F29A8C</SolidColorBrush>
    <SolidColorBrush x:Key="ForegroundColor">#212A37</SolidColorBrush>
    <SolidColorBrush x:Key="BackgroundColor">#FFFFFF</SolidColorBrush>
</ResourceDictionary>