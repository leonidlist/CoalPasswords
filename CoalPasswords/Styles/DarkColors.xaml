        <GradientStop Color="#0E1621" Offset="0.2"></GradientStop>
        <GradientStop Color="#0E1621" Offset="0.7"></GradientStop>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="AccentColor">#F29A8C</SolidColorBrush>
